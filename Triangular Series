public class five //Class name
{
public static void main(String args[])
{
for(int i=0; i<=10; i++) //Loop for the number of rows
{
for( int j=0; j<=i; j++)// Loop for number of columns
{
System.out.print(j); //Prints the series for a right angled triangle
}
System.out.println(); //Used so that the compiler can go to the next line when the loop does not stand true
}
}
}
