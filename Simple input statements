import java.io.*;  /* Used to specify the package being used*/
import java.util.*;
class one
{
public static void main(String args[]) throws IOException
{
int a,b; /* Declaration on the variables as well as specifying their types Here the integer type of data is being used
char f,m;   Here the character type of data is being used/*
BufferedReader br= new BufferedReader( new InputStreamReader(System.in)); //Creating an object for the Buffered Reader
System.out.println("Please enter your first integer"); // Asking you to do a task
a= Integer.parseInt(br.readline()); //Storing your answer in the variables assigned
System.out.println("Please enter your second integer");
b=Integer.parseInt(br.readLine());
System.out.println(" Please enter a character to store these values");
f= (char)br.read();
System.out.println("Your character inputted is "+f); // The +f will print the value of whatever is stored in the variable f

System.out.println(Now the two values inputted are" +a+  +b);
}
}
